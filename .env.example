# MongoDB Connection
# Get this from Railway MongoDB service → Variables → MONGO_URL
MONGODB_URI=your_mongodb_connection_string_here

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-3-flash-preview
GEMINI_IMAGE_MODEL=gemini-3-pro-image-preview

# Server Configuration
PORT=8080
NODE_ENV=production

# Frontend Origins (for CORS)
# Set to your Vercel frontend URL
FRONTEND_ORIGIN=https://dpal-front-end.vercel.app
FRONTEND_ORIGIN_2=

# Note: Railway will auto-detect these variables.
# Make sure to set the correct values in Railway → Variables tab.
# Localhost values are only for local development.
